LXI H,2050
MOV A,M
MOV C,M 
CPI 06    ;CUT FROM HERE 
JZ CASE6
CPI 07
JZ CASE7  ;TO HERE
MVI B,00
DCR C
JZ ANS
OUTERLOOP: MOV E,A
MOV D,C
DCR D
JZ ANS
LOOPINNER: ADD E
JNC CARRY
INR B
CARRY: DCR D
JNZ LOOPINNER
DCR C
JZ ANS
JNZ OUTERLOOP
ANS: INX H
MOV M,A
INX H
MOV M,B
HLT
CASE6: INX H    ;CUT FROM HERE 
MVI A,208
MOV M,A
INX H
MVI A,02
MOV M,A
HLT
CASE7: INX H
MVI A,176
MOV M,A
INX H
MVI A,19
MOV M,A
HLT             ;TO HERE